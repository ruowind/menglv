{
  "name": "menglv",
  "version": "0.0.1",
  "description": "A desktop application for lvmama fed",
  "main": "main.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open --hot",
    "build:web": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "start": "electron .",
    "build": "rimraf dist && electron-packager ./ MengLv --platform=win32 --arch=x64 --overwrite --out ./dist --electron-version=1.7.9 --icon=assets/app.ico --ignore=\"(.idea|.node|.vscode|dist|.eslintrc|.gitignore|LICENSE|package-lock.json|README.md)\"",
    "build:win64": "rimraf release && electron-packager ./ MengLv --platform=win32 --arch=x64 --overwrite --out ./release --electron-version=1.7.9 --icon=assets/app.ico"
  },
  "repository": "https://github.com/ruowind/menglv",
  "keywords": [
    "Electron",
    "Lvmama"
  ],
  "author": "GengFeng",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "electron": "1.7.9",
    "electron-packager": "^9.1.0",
    "electron-rebuild": "^1.6.0",
    "file-loader": "^1.1.5",
    "node-sass": "^4.5.3",
    "sass-loader": "^6.0.6",
    "vue": "^2.4.4",
    "vue-loader": "^13.3.0",
    "vue-template-compiler": "^2.4.4",
    "webpack": "^3.7.1",
    "webpack-dev-server": "^2.9.1"
  },
  "dependencies": {
    "async": "^2.5.0",
    "async-foreach": "^0.1.3",
    "base64-img": "^1.0.3",
    "bindings": "^1.3.0",
    "browser-sync": "^2.18.13",
    "chalk": "^2.1.0",
    "cross-spawn": "^5.1.0",
    "cssbeautify": "^0.3.1",
    "decree": "0.0.6",
    "gaze": "^1.1.2",
    "get-stdin": "^5.0.1",
    "glob": "^7.1.2",
    "gulp": "^3.9.1",
    "gulp-clean-css": "^3.9.0",
    "gulp-less": "^3.3.2",
    "gulp-sourcemaps": "^2.6.1",
    "gulp-uglify": "^3.0.0",
    "gulp-util": "^3.0.8",
    "html2js": "^0.2.0",
    "in-publish": "^2.0.0",
    "jquery": "^3.2.1",
    "lodash": "^4.17.4",
    "lodash.assign": "^4.2.0",
    "lodash.clonedeep": "^4.5.0",
    "lodash.mergewith": "^4.6.0",
    "meow": "^3.7.0",
    "mkdirp": "^0.5.1",
    "nan": "^2.7.0",
    "npmlog": "^4.1.2",
    "rc": "^1.2.1",
    "request": "^2.83.0",
    "rimraf": "^2.6.2",
    "sass-graph": "^2.2.4",
    "semver": "^5.4.1",
    "stdout-stream": "^1.4.0",
    "through2": "^2.0.3",
    "vinyl-sourcemaps-apply": "^0.2.1",
    "watchr": "^3.0.1"
  }
}
