{
  "name": "menglv",
  "version": "0.0.1",
  "description": "A desktop application for lvmama fed",
  "main": "main.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open --hot",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "start": "electron .",
    "build:win64": "rimraf dist && electron-packager ./ MengLv --platform=win32 --arch=x64 --overwrite --out ./dist --electron-version=1.6.11 --icon=assets/app.ico"
  },
  "repository": "https://github.com/ruowind/menglv",
  "keywords": [
    "Electron",
    "Lvmama"
  ],
  "author": "GengFeng",
  "license": "MIT",
  "devDependencies": {
    "vue": "^2.4.2",
    "electron": "1.6.11",
    "electron-packager": "^8.7.2",
    "electron-rebuild": "^1.6.0",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-env": "^1.6.0",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.4",
    "file-loader": "^0.11.2",
    "sass-loader": "^6.0.6",
    "less-loader": "^4.0.5",
    "vue-loader": "^13.0.2",
    "vue-template-compiler": "^2.4.2",
    "webpack": "^3.4.1",
    "webpack-dev-server": "^2.6.1"
  },
  "dependencies": {
    "base64-img": "^1.0.3",
    "browser-sync": "^2.18.13",
    "chalk": "^2.0.1",
    "cssbeautify": "^0.3.1",
    "gulp": "^3.9.1",
    "gulp-clean-css": "^3.7.0",
    "gulp-less": "^3.3.2",
    "gulp-sourcemaps": "^2.6.0",
    "gulp-uglify": "^3.0.0",
    "gulp-util": "^3.0.8",
    "gulp-sass": "^3.1.0",
    "node-lwip": "^0.2.1",
    "html2js": "^0.2.0",
    "jquery": "^3.2.1",
    "lodash": "^4.17.4",
    "rimraf": "^2.6.1",
    "through2": "^2.0.3",
    "watchr": "^3.0.1"
  }
}